<div>
    <h1>{{name}}</h1>
    <p>
        <b>Bithdate:</b>
        <date>{{birthdate}}</date>
    </p>
    <p><b>Bithplace:</b> {{birthplace}}
    </p>
    <p><b>Deathdate:</b>
        <date>{{deathdate}}</date>
    </p>
    <p><b>Deathplace:</b> {{deathplace}}</p>
    <p><b>Nationality:</b> {{nationality}}</p>
    <p><b>Occupation:</b> {{occupation}}</p>
    <p><b>We know him for:</b></p>
    <ul>
        {{#each known_for}}
        <li>{{this}}<br></li>
        {{/each}}
    </ul>
    <br>
    <hr>
    <p><b>Education:</b> {{#each education}}</p>
    <ul>
        <li>Degree: {{degree}}</li>
        <li>Field: {{field}}</li>
        <li>Institution: {{institution}}</li>
        <li>Year: {{year}}</li>
    </ul>
    {{/each}}
    <br>
    <hr>
    <p><b>Notable publications:</b> {{#each notable_publications}}</p>
    <ul>
        <li>Title: "{{title}}"</li>
        <li>Year: {{year}}</li>
        <li>Publisher: {{publisher}}</li>
    </ul>
    {{/each}}
    <br>
    <hr>
    <p><b>Partners: {{#each partners}}</b></p>
    <ul>
        <li>Name: {{name}}</li>
        <li>Relationship: {{relationship}}</li>
        <li>Years: {{years}}</li>
    </ul>
    {{/each}}
    <br>
    <hr>
    <h4>Awards: {{#each awards}}</h4>
    <ul>
        <li>Title: "{{title}}"</li>
        <li>Year: {{year}}</li>
        <li>Description: {{description}}</li>
    </ul>
    {{/each}}
    <br>
    <hr>
    <h4>Influences:</h4>
    <ul>
        {{#each influences}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <hr>
    <h4>Influenced:</h4>
    <ul>
        {{#each influenced}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <hr>
    <h4>Quotes:</h4>
    <ul>
        {{#each quotes}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <hr>
    <h4>Major discaveries:</h4>
    <ul>
        {{#each major_discoveries}}
        <li>Title: "{{title}}"</li>
        <li>Year: {{year}}</li>
        <li>Description: {{description}}</li>
        {{/each}}
    </ul>
    <br>
    <hr>
    <h4>Contributions:</h4>
    <ul>
        {{#with contributions}}
        <li>Title: "{{title}}"</li>
        <li>Description: {{description}}</li>
        <li>Year: {{year}}</li>
        <li>Field: {{field}}</li>
        {{/with}}
    </ul>
</div>